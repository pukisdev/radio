			"contents": "PROCEDURE POSTING_GL_PENERIMAAN_DUA IS\nvPESAN			NUMBER;\nCURSOR C_NP_MST IS   \n       SELECT A.LKS,\n              A.NO_BUKTI,\n              A.TGL_TERIMA,\n              NVL(A.TOTAL,0),\n              A.KETERANGAN\n       FROM 	BI_KEU.FA_PENERIMAAN_MST A\n       WHERE  A.TGL_TERIMA BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE);\n       --WHERE  (TO_CHAR(A.TGL_TERIMA,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY'));   --(A.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE) \n       --AND    A.POSTING LIKE '0';\n             \nCURSOR C_NP_DET IS   \n       SELECT A.NO_BUKTI,\n              A.COA_ID,\n              A.CURRENCY_ID,\n              A.TIPE,\n              A.NILAI,\n              B.LKS,\n              A.KETERANGAN\n       FROM 	BI_KEU.FA_PENERIMAAN_MST B,BI_KEU.FA_PENERIMAAN_JURNAL_VIEW A\n       WHERE 	B.NO_BUKTI = A.NO_BUKTI \n       --AND		B.POSTING LIKE '0' \n       AND    B.TGL_TERIMA BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE);\n       --AND    (TO_CHAR(B.TGL_TERIMA,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY')); --(B.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE);\n                  \n                  \n\nCURSOR C_KAR IS   \n       SELECT NO_BUKTI,\n   			      COA_ID,\n       			  NO_FAKTUR,\n    			    CURRENCY_ID,\n       			  KETERANGAN,\n       				NILAI_BAYAR,\n       				TGL_CETAK\n   			  --    URUTAN\n		   FROM   BI_KEU.FA_KARTU_PIUTANG_BI_VIEW A\n		   WHERE  A.TGL_CETAK BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE);\n       --WHERE (TO_CHAR(A.TGL_CETAK,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY'));  --(A.TGL_CETAK BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE);\n     \n/*\n     CURSOR C_KAR IS\n         SELECT A.NO_BUKTI,A.COA_ID,A.NO_FAKTUR,A.CURRENCY_ID,B.KETERANGAN,A.NILAI_BAYAR,B.TGL_CETAK\n         FROM   FA_PENERIMAAN_AR_DET A, FA_PENERIMAAN_MST B\n         WHERE  A.NO_BUKTI = B.NO_BUKTI\n    AND    B.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE;\n*/\n\nCURSOR C_KARTU_HUTANG IS   \n       SELECT NO_BUKTI,\n   			      COA_ID,\n       			  NO_INVOICE,\n    			    CURRENCY_ID,\n       			  KETERANGAN,\n       				NILAI_BAYAR,\n       				TGL_TERIMA\n   			  --    URUTAN\n		   FROM   BI_KEU.FA_KARTU_HUTANG_PENERIMAAN_V A\n		   WHERE  A.TGL_TERIMA BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE);\n       --WHERE  TO_CHAR(A.TGL_TERIMA,'MM/YYYY') = :MAIN.PERIODE;  --(A.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE);\n      \n     \nCURSOR C_FAK IS   \n       SELECT TRIM(A.NO_FAKTUR) NO_FAKTUR,\n              A.CURRENCY_ID,\n        --    A.KETERANGAN,\n              SUM(A.NILAI_BAYAR) NILAI_BAYAR\n              --B.TGL_TERIMA\n        FROM 	FA_PENERIMAAN_AR_DET A,FA_PENERIMAAN_MST B\n     		WHERE A.NO_BUKTI = B.NO_BUKTI \n           -- A.NO_FAKTUR='003510808' AND\n        AND   B.TGL_TERIMA BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE)\n        --AND		TO_CHAR(B.TGL_TERIMA,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY')  --(B.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE)\n        --AND   B.POSTING LIKE '0'\n        GROUP BY A.NO_FAKTUR,\n            		 A.CURRENCY_ID;\n            --A.KETERANGAN,\n            --B.TGL_TERIMA;\n                       \n     \nCURSOR C_SAL IS   \n       SELECT B.COA_CUSTOMER,\n              SUM(B.NILAI_BAYAR) TOTAL,\n              'K'||TO_CHAR(TO_NUMBER(TO_CHAR(A.TGL_TERIMA,'mm'))),\n              TO_CHAR(A.TGL_TERIMA,'yyyy')\n        FROM 	BI_KEU.FA_PENERIMAAN_MST A,BI_KEU.FA_PENERIMAAN_AR_DET B\n        WHERE A.NO_BUKTI = B.NO_BUKTI --AND\n              --A.NO_BUKTI = XNO_BUKTI;\n              --  AND B.COA_CUSTOMER  ='1.1.03.06.I0005'\n        AND   A.TGL_TERIMA BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE)\n        --AND 	TO_CHAR(A.TGL_TERIMA,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY')  --(A.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE) \n        --AND 	A.POSTING LIKE '0'\n        AND 	B.COA_CUSTOMER IS NOT NULL\n        GROUP BY B.COA_CUSTOMER,\n                 'K'||TO_CHAR(TO_NUMBER(TO_CHAR(A.TGL_TERIMA,'mm'))),\n                 TO_CHAR(A.TGL_TERIMA,'yyyy');            \n\n                         \nCURSOR C_SET_MST IS                     \n       SELECT A.NO_BUKTI,\n              A.TGL_CETAK,\n              A.JUMLAH,\n              A.KETERANGAN\n     	 FROM 	FA_SETORAN_BANK_MST A\n     	 WHERE  A.TGL_CETAK BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE);\n     	 --WHERE TO_CHAR(A.TGL_CETAK,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY');   --(A.TGL_CETAK BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE) \n       --AND A.POSTING LIKE '0';\n                \nCURSOR C_SET_DET IS  \n       SELECT B.COA_ID,\n         			B.NO_BUKTI,\n       	 			B.DEBET,\n       				B.KREDIT,\n       				B.KETERANGAN\n		   FROM 	FA_SETORAN_BANK_MST A, FA_SETORAN_BANK_JURNAL_DET B\n		 	 WHERE 	A.NO_BUKTI =B.NO_BUKTI\n		 	 AND    A.TGL_CETAK BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE);\n 		 	 --AND  	TO_CHAR(A.TGL_CETAK,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY');  --(A.TGL_CETAK BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE) \n     	 --AND 		A.POSTING LIKE '0';\n     \n \n       VNO_BUKTI            VARCHAR2(25);\n       VNO_BUKTI_MST        VARCHAR2(25);\n       VTGL_TERIMA          DATE;\n       VTGL_TERIMA_MST  		DATE;\n       VCOA_ID_CUST         VARCHAR2(30);\n       VTOTAL               NUMBER(16,2);\n       VTOTAL_MST           NUMBER(16,2);\n       VKETERANGAN          VARCHAR2(300);\n       VKETERANGAN_MST    	VARCHAR2(300);       \n       VLKS                 VARCHAR2(5);\n       VTIPE                VARCHAR2(1);\n       VNILAI               NUMBER(16,2);     \n       VNO_BUKTI_DET        VARCHAR2(25);     \n       VCOA_ID              VARCHAR2(30);   \n       VCURRENCY_ID         VARCHAR2(15);\n       VDEBET               NUMBER(16,2);\n       VKREDIT              NUMBER(16,2);\n       VPROD_ID             NUMBER(2);\n       VKODE_PERWAKILAN 		VARCHAR2(5);\n       VPOSTING             VARCHAR2(1);\n       VLEDGER_DET          NUMBER;\n       VLEDGER_ISI_DET    	NUMBER;       \n       VLEDGER_MST          NUMBER;\n       VLEDGER_ISI_MST    	NUMBER;       \n       VJENIS_BUKTI         VARCHAR2(1);\n       VJURNAL              VARCHAR2(2);\n       VJMLKODE             NUMBER;\n       VCOUNT               NUMBER;\n       \n       \n       VNO_BUKTI_KAR        VARCHAR2(30);     \n       VNO_FAKTUR_KAR       VARCHAR2(30);\n       VCOA_CUSTOMER_KAR    VARCHAR2(30); \n       VCURRENCY_ID_KAR    	VARCHAR2(15);\n       VTIPE_KAR            VARCHAR2(1);\n       VNILAI_KAR           NUMBER(16,2);\n       VLKS_KAR            	VARCHAR2(5);\n       VKETERANGAN_KAR      VARCHAR2(300);\n       VTGL_TERIMA_KAR      DATE;\n       VRANK_KAR           	NUMBER;\n        \n        \n      VNO_BUKTI_FAK         VARCHAR2(30);     \n      VNO_FAKTUR_FAK        VARCHAR2(30);\n      VCOA_CUSTOMER_FAK    	VARCHAR2(30); \n      VCURRENCY_ID_FAK    	VARCHAR2(15);\n      VTIPE_FAK            	VARCHAR2(1);\n      VNILAI_FAK            NUMBER(16,2);\n      VLKS_FAK            	VARCHAR2(5);\n      VKETERANGAN_FAK       VARCHAR2(300);\n      VTGL_TERIMA_FAK       DATE;\n      VBAYAR_FAK            NUMBER(16,2);\n               \n               \n           \n      VNO_BUKTI_SAL         VARCHAR2(30);\n      VTGL_TERIMA_SAL       DATE;\n      VCOA_CUSTOMER_SAL    	VARCHAR2(30);\n      VCOA_ID_SAL           VARCHAR2(30);\n      VTOTAL_SAL            NUMBER(16,2);\n      VLKS_SAL            	VARCHAR2(5);\n      VKETERANGAN_SAL       VARCHAR2(300);\n      VBULAN_SAL            VARCHAR2(4);\n      VTAHUN_SAL            VARCHAR2(4);\n      VD1                		NUMBER(16,2);\n      VD2                		NUMBER(16,2);\n      VD3                		NUMBER(16,2);\n      VD4                		NUMBER(16,2);\n      VD5                		NUMBER(16,2);\n      VD6                		NUMBER(16,2);\n      VD7                		NUMBER(16,2);\n      VD8                		NUMBER(16,2);\n      VD9                		NUMBER(16,2);\n      VD10           				NUMBER(16,2);\n      VD11            			NUMBER(16,2);\n      VD12            			NUMBER(16,2);\n      VK1                		NUMBER(16,2);\n      VK2                		NUMBER(16,2);\n      VK3                		NUMBER(16,2);\n      VK4                		NUMBER(16,2);\n      VK5                		NUMBER(16,2);\n      VK6                		NUMBER(16,2);\n      VK7                		NUMBER(16,2);\n      VK8                		NUMBER(16,2);\n      VK9                		NUMBER(16,2);\n      VK10            			NUMBER(16,2);\n      VK11            			NUMBER(16,2);\n      VK12            			NUMBER(16,2);    \n        \n      ---------  SETORAN --------------------------   \n        \n      VNO_BUKTI_SEL         VARCHAR2(25);\n      VNO_BUKTI_MST_SEL     VARCHAR2(25);\n      VTGL_TERIMA_SEL       DATE;\n      VTGL_TERIMA_MST_SEL  	DATE;\n      VCOA_ID_CUST_SEL      VARCHAR2(30);\n      VTOTAL_SEL            NUMBER(16,2);\n      VTOTAL_MST_SEL        NUMBER(16,2);\n      VKETERANGAN_SEL       VARCHAR2(300);\n      VKETERANGAN_MST_SEL   VARCHAR2(300);       \n      VLKS_SEL              VARCHAR2(5);\n      VTIPE_SEL             VARCHAR2(1);\n      VNILAI_SEL_D          NUMBER(16,2);     \n      VNILAI_SEL_K          NUMBER(16,2);     \n      VNO_BUKTI_DET_SEL     VARCHAR2(25);     \n      VCOA_ID_SEL           VARCHAR2(30);   \n      VCURRENCY_ID_SEL      VARCHAR2(15);\n      VDEBET_SEL            NUMBER(16,2);\n      VKREDIT_SEL           NUMBER(16,2);\n      VPROD_ID_SEL          NUMBER(2);\n      VKODE_PERWAKILAN_SEL 	VARCHAR2(5);\n      VPOSTING_SEL          VARCHAR2(1);\n      VLEDGER_DET_SEL       NUMBER;\n      VLEDGER_ISI_DET_SEL   NUMBER;       \n      VLEDGER_MST_SEL       NUMBER;\n      VLEDGER_ISI_MST_SEL   NUMBER;       \n      VJENIS_BUKTI_SEL      VARCHAR2(1);\n      VJURNAL_SEL           VARCHAR2(2);\n      VJMLKODE_SEL          NUMBER;\n      VCOUNT_SEL            NUMBER;\n        \n        \n               \n\nBEGIN\n  BEGIN                                 \n  OPEN C_NP_MST;\n  LOOP\n    FETCH C_NP_MST INTO VKODE_PERWAKILAN,\n                        VNO_BUKTI_MST,\n                        VTGL_TERIMA_MST,\n                        VTOTAL_MST,\n                        VKETERANGAN_MST;\n                \n  EXIT WHEN C_NP_MST%NOTFOUND;\n      \n  BEGIN\n       SELECT MAX(NVL(RECORD_NUMBER,0))+1\n       INTO 	VLEDGER_ISI_MST\n       FROM 	BI_ACC.ACC_GL_TRANS_MST;\n       \n       IF VLEDGER_ISI_MST = 0 OR VLEDGER_ISI_MST IS NULL THEN\n              VLEDGER_MST := 1;\n       ELSE\n              VLEDGER_MST := NVL(VLEDGER_ISI_MST,0)+1;\n       END IF;       \n  EXCEPTION\n         WHEN NO_DATA_FOUND THEN VLEDGER_MST := 0;       \n  END;\n  BEGIN\n      SELECT 	DISTINCT A.JENIS_BUKTI \n      INTO 	 	VJENIS_BUKTI\n      FROM 	 	BI_KEU.FA_PENERIMAAN_MST A\n      WHERE   (A.TGL_TERIMA BETWEEN TRUNC(:MAIN.PERIODE,'MM') AND LAST_DAY(:MAIN.PERIODE))\n      --WHERE 	TO_CHAR(A.TGL_TERIMA,'MM/YYYY') = TO_CHAR(:MAIN.PERIODE,'YYYY')  --(A.TGL_TERIMA BETWEEN :MAIN.FROM_DATE AND :MAIN.TO_DATE) \n      AND 		A.NO_BUKTI LIKE VNO_BUKTI_MST;\n\n      IF VJENIS_BUKTI LIKE 'K' THEN\n         VJURNAL := '01';\n      ELSIF\n         VJENIS_BUKTI LIKE 'B' THEN\n         VJURNAL := '03';\n      ELSIF\n         VJENIS_BUKTI LIKE 'G' THEN\n         VJURNAL := '05';\n      END IF;       \n  EXCEPTION\n  		WHEN NO_DATA_FOUND THEN NULL;    	\n  END;\n  \n  BEGIN\n      SELECT COUNT(*)\n      INTO 	 VJMLKODE\n      FROM 	 BI_ACC.ACC_GL_TRANS_MST\n      WHERE  BUKTI = VNO_BUKTI_MST \n      AND		 LKS   = VKODE_PERWAKILAN;\n  EXCEPTION\n      WHEN NO_DATA_FOUND THEN VJMLKODE := 0;\n  END;\n                  \n    IF VJMLKODE = 0 OR VJMLKODE IS NULL THEN          \n         INSERT INTO BI_ACC.ACC_GL_TRANS_MST(LKS,\n                                             BUKTI,\n                                             TGL,\n                                             JURNAL,\n                                             TDEBET,\n                                             TKREDIT,\n                                             POSTING,\n                                             RECORD_NUMBER,\n                                             PRODUCT_ID)\n         VALUES (VKODE_PERWAKILAN,\n                 VNO_BUKTI_MST,\n                 VTGL_TERIMA_MST,\n                 VJURNAL,\n                 VTOTAL_MST,\n                 VTOTAL_MST,\n                 'N',\n                 VLEDGER_MST,\n                 1);--VPROD_ID);\n         COMMIT;\n         CLEAR_MESSAGE;\n         :CONTROL.DISPLAY := 'Proses Untuk Nomor Bukti : '||VNO_BUKTI_MST;\n         SYNCHRONIZE;\n    ELSE\n    		 BEGIN\n	         UPDATE BI_ACC.ACC_GL_TRANS_MST \n	         SET 		TDEBET = VTOTAL_MST,TKREDIT = VTOTAL_MST\n	         WHERE 	BUKTI = VNO_BUKTI_MST \n	         AND    LKS   = VKODE_PERWAKILAN;\n    		 EXCEPTION\n    		 	 WHEN NO_DATA_FOUND THEN NULL;\n    		 END;\n         \n         CLEAR_MESSAGE;\n         :CONTROL.DISPLAY := 'Proses Untuk Nomor Bukti : '||VNO_BUKTI_MST;\n         SYNCHRONIZE;                                  \n    END IF;        \n  END LOOP;\n  CLOSE C_NP_MST;\nEND; \n\n\n                ---=====================================================\n                ---ISI DETAIL GENERAL LEDGER (TABLE ACC_GL_TRANS_DET)\n                ---===================================================== \nBEGIN                                 \n  OPEN C_NP_DET;\n  LOOP\n    FETCH C_NP_DET INTO VNO_BUKTI_DET,\n                        VCOA_ID,\n                        VCURRENCY_ID,\n                        VTIPE,\n                        VNILAI,\n                        VLKS,\n                        VKETERANGAN;\n                        \n  	EXIT WHEN C_NP_DET%NOTFOUND;\n\n  	BEGIN\n    		SELECT 	MAX(NVL(RECORD_NUMBER,0))\n        INTO 		VLEDGER_ISI_DET\n        FROM 		BI_ACC.ACC_GL_TRANS_DET\n        WHERE 	LKS = VLKS;\n        \n        IF VLEDGER_ISI_DET = 0 OR VLEDGER_ISI_DET IS NULL THEN\n        		VLEDGER_DET := 1;\n        ELSE\n        		VLEDGER_DET := NVL(VLEDGER_ISI_DET,0)+1;\n        END IF;\n     EXCEPTION\n     		WHEN NO_DATA_FOUND THEN VLEDGER_DET := 0;       \n     END;\n     \n  	 -- MASUK KE GL TRANSAKSI DEBET DAN KREDIT\n  	 INSERT INTO BI_ACC.ACC_GL_TRANS_DET(LKS,\n                                         REK,\n                                         BUKTI,\n                                         URAI,\n                                         DK,\n                                         NILAI,\n                                         RECORD_NUMBER)\n      VALUES(VLKS,\n             VCOA_ID,\n             VNO_BUKTI_DET,\n             VKETERANGAN,\n             VTIPE,\n             VNILAI,\n             VLEDGER_DET);\n                                           \n    	COMMIT;\n      CLEAR_MESSAGE;\n      :CONTROL.DISPLAY := 'Proses Untuk Nomor Bukti : '||VNO_BUKTI_DET;\n      SYNCHRONIZE;                                    \n  END LOOP;\n  CLOSE C_NP_DET;\nEND;\n  \n                  ---=====================================================\n                ---ISI DETAIL GENERAL LEDGER (TABLE ACC_KARTU_PIUTANG)\n                ---===================================================== \nBEGIN                                 \n  OPEN C_KAR;\n  LOOP\n    FETCH C_KAR INTO VNO_BUKTI_KAR,\n                     VCOA_CUSTOMER_KAR,  \n                     VNO_FAKTUR_KAR,\n                     VCURRENCY_ID_KAR,\n                     VKETERANGAN_KAR,\n                     VNILAI_KAR,\n                     VTGL_TERIMA_KAR;\n                   -- VRANK_KAR;\n\n                        \n  	EXIT WHEN C_KAR%NOTFOUND;\n\n\n	  -- MASUK KE GL TRANSAKSI DEBET DAN KREDIT\n  	INSERT INTO BI_ACC.ACC_KARTU_PIUTANG(COA_ID,\n                                         TGL,\n                                         NO_BUKTI,\n                                         DK,\n                                         KETERANGAN,\n                                         NILAI,\n                                         CURRENCY,\n                                         NILAI_TUKAR,\n                                         TIPE)\n     VALUES(VCOA_CUSTOMER_KAR,\n            VTGL_TERIMA_KAR,\n            VNO_BUKTI_KAR,\n            'K',\n            VKETERANGAN_KAR,\n            VNILAI_KAR,\n            VCURRENCY_ID_KAR,\n            1,4);\n                                           \n    	COMMIT;\n    \n    /*	\n  	INSERT INTO BI_ACC.ACC_RK_AGEN(COA_ID,\n                                   TGL,\n                                   NO_BUKTI,\n                                   DK,\n                                   KETERANGAN,\n                                   NILAI,\n                                   CURRENCY,\n                                   NILAI_TUKAR,\n                                   TIPE)\n     VALUES(VCOA_CUSTOMER_KAR,\n            VTGL_TERIMA_KAR,\n            VNO_BUKTI_KAR,\n            'K',\n            VKETERANGAN_KAR,\n            VNILAI_KAR,\n            VCURRENCY_ID_KAR,\n            1,4);\n                                           \n    	COMMIT;    	\n    	*/\n    	\n      CLEAR_MESSAGE;\n      :CONTROL.DISPLAY := 'Proses Untuk Nomor Bukti : '||VNO_BUKTI_KAR;\n      SYNCHRONIZE;                                    \n    END LOOP;\n    CLOSE C_KAR;\nEND;\n\n\n                 ---=====================================================\n                ---ISI DETAIL GENERAL LEDGER (TABLE ACC_SALDO_PIUTANG)\n                ---===================================================== \n\nBEGIN                                 \n  OPEN C_SAL;\n  LOOP\n    FETCH C_SAL INTO VCOA_CUSTOMER_SAL, \n                     VTOTAL_SAL,\n                     VBULAN_SAL,\n                     VTAHUN_SAL;\n\n                        \n    EXIT WHEN C_SAL%NOTFOUND;\n\n\n  	-- MASUK KE GL TRANSAKSI DEBET DAN KREDIT\n		BEGIN\n  			SELECT COA_ID,\n         			 TAHUN,\n         			 K1,\n         			 K2,\n         			 K3,\n         			 K4,\n         			 K5,\n         			 K6,\n         			 K7,\n         			 K8,\n         			 K9,\n         			 K10,\n         			 K11,\n         			 K12\n   			INTO   VCOA_ID_SAL,\n            	 VTAHUN_SAL, \n            	 VK1,\n         			 VK2,\n         			 VK3,\n         			 VK4,\n         			 VK5,\n         			 VK6,\n         			 VK7,\n         			 VK8,\n         			 VK9,\n         			 VK10,\n         			 VK11,\n         			 VK12\n   			FROM 	 BI_ACC.ACC_SALDO_PIUTANG\n   			WHERE  COA_ID = VCOA_CUSTOMER_SAL\n        AND    TAHUN  = VTAHUN_SAL;\n		EXCEPTION\n	    	WHEN NO_DATA_FOUND THEN NULL;\n	 	END;\n\n   BEGIN       \n		   IF VBULAN_SAL = 'K1' THEN                                \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   	K1 = NVL(VK1,0) + NVL(VTOTAL_SAL,0)\n		               WHERE 	COA_ID = VCOA_CUSTOMER_SAL\n		               AND   	TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		      VBULAN_SAL = 'K2' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   	K2 = NVL(VK2,0) + NVL(VTOTAL_SAL,0)\n		               WHERE 	COA_ID = VCOA_CUSTOMER_SAL\n		               AND   	TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		      VBULAN_SAL = 'K3' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   	K3 = NVL(VK3,0) + NVL(VTOTAL_SAL,0)\n		               WHERE 	COA_ID = VCOA_CUSTOMER_SAL\n		               AND   	TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K4' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K4 = NVL(VK4,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K5' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K5 = NVL(VK5,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K6' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K6 = NVL(VK6,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K7' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K7 = NVL(VK7,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K8' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K8 = NVL(VK8,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K9' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K9 = NVL(VK9,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K10' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K10 = NVL(VK10,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K11' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K11 = NVL(VK11,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		   ELSIF\n		         VBULAN_SAL = 'K12' THEN          \n		               UPDATE BI_ACC.ACC_SALDO_PIUTANG\n		               SET   K12 = NVL(VK12,0) + NVL(VTOTAL_SAL,0)\n		               WHERE COA_ID = VCOA_CUSTOMER_SAL AND\n		              TAHUN  = VTAHUN_SAL;\n		    END IF;                \n                                           \n		    COMMIT;\n		    CLEAR_MESSAGE;\n		    :CONTROL.DISPLAY := 'Proses Untuk Nomor Bukti : '||VNO_BUKTI_KAR;\n		    SYNCHRONIZE;\n		EXCEPTION\n				WHEN NO_DATA_FOUND THEN NULL;   	\n		END;                                    \n    END LOOP;\n   CLOSE C_SAL;\n\nEND;\n\n\n                    ---=====================================================\n                ---ISI DETAIL GENERAL LEDGER (TABLE ACC_SETORAN_BANK)\n                ---===================================================== \n BEGIN                                 \n  OPEN C_SET_MST;\n  LOOP\n    FETCH C_SET_MST INTO VNO_BUKTI_MST_SEL,\n                                            VTGL_TERIMA_MST_SEL,\n                                            VTOTAL_MST_SEL,\n                                            VKETERANGAN_MST_SEL;\n\n                        \n  EXIT WHEN C_SET_MST%NOTFOUND;\n\n           \n  -- MASUK KE GL TRANSAKSI DEBET DAN KREDIT\n  INSERT INTO BI_ACC.ACC_GL_TRANS_MST(LKS,\n                                             BUKTI,\n                                                                                TGL,\n                                                                                JURNAL,\n                                                                                TDEBET,\n                                                                                TKREDIT,\n                                                                                POSTING,\n                                                                                RECORD_NUMBER,\n                                                                                PRODUCT_ID)\n                                                                    VALUES ('AGP',\n                                                                                VNO_BUKTI_MST_SEL,\n                                                                            VTGL_TERIMA_MST_SEL,\n                                                                                    '07',\n                                                                                    VTOTAL_MST,\n                                                                                    VTOTAL_MST,\n                                                                                    'N',\n                                                                                    NULL,\n                                                                                    1);--VPROD_ID);\n                                           \n    COMMIT;\n                         CLEAR_MESSAGE;\n                    :CONTROL.DISPLAY := 'Proses Untuk Nomor Bukti : '||VNO_BUKTI_MST_SEL;\n                    SYNCHRONIZE;                                    \n     END LOOP;\n    CLOSE C_SET_MST;\n  \n\n  END;\n  \n  \n                  ---=====================================================\n                ---ISI DETAIL GENERAL LEDGER (TABLE ACC_SETORAN_BANK_AP_DET)\n                ---===================================================== \n  BEGIN                                 \n  OPEN C_SET_DET;\n  LOOP\n    FETCH C_SET_DET INTO     VCOA_ID_SEL,\n                          VNO_BUKTI_DET_SEL,\n                                                  VNILAI_SEL_D,\n                                                    VNILAI_SEL_K,\n                                                  VKETERANGAN_SEL;\n                                                \n                                                \n        \n                        \n  EXIT WHEN C_SET_DET%NOTFOUND;\n  \n  IF VNILAI_SEL_K =0 AND VNILAI_SEL_D > 0 THEN\n  		-- MASUK KE GL TRANSAKSI DEBET DAN KREDIT\n  		INSERT INTO BI_ACC.ACC_GL_TRANS_DET(LKS,\n                                          REK,\n                                          BUKTI,\n                                          URAI,\n                                          DK,\n                                          NILAI,\n                                          RECORD_NUMBER)\n       VALUES('AGP',\n              VCOA_ID_SEL,\n              VNO_BUKTI_DET_SEL,\n              VKETERANGAN_SEL,\n              'D',\n              VNILAI